<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="world.festival.dao.ListMapper">

<!-- 축제등록 -->	
<insert id="writeFestival" parameterType="ListVO">
	INSERT INTO
		mainboard
	VALUES (
		#{userid},
		#{title},
		#{country},
		#{adress},
		#{festival_intro},
		#{startEvent},
		#{endEvent}
	)
</insert>


</mapper>